module rule-overview

imports libstrategolib

	
rules
	E : Not(True())      -> False()       
  	E() : Not(False())     -> True()
  
    E(s) : Not(True())      -> False()       
  	E(s1,s2) : Not(False())     -> True()
  	
    E(|t) : Not(True())      -> False()       
  	E(|t1,t2) : Not(False())     -> True()
	
	E(s|t) : Not(True())      -> False()
	 
  	E : Not(False())     -> out
  		with out := True()
  		
  	E : Not(False())     -> out
  		where out := True()

  	E : Not(False())     -> out
  		where( out := True() )
  		
  	E : Not(False())     -> out'
  		with 
  			out := False() ;
  			out' := out
  		
